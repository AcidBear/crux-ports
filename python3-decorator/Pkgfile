name=python3-decorator
version=4.0.10
release=1
source=(https://pypi.python.org/packages/13/8a/4eed41e338e8dcc13ca41c94b142d4d20c0de684ee5065523fee406ce76f/decorator-4.0.10.tar.gz#md5=434b57fdc3230c500716c5aff8896100)

build()
{
	cd decorator-$version
	python3 setup.py --root=$PFG
}
