# Description: Video editing with Python.
# URL: https://pypi.python.org/pypi/moviepy
# Maintainer: Jake Slazenger jake dot slazenger at yandex dot com
# Depends on: python3 imagemagick python3-decorator pyhton3-tqdm python3-numpy python3-imageio

name=python3-moviepy
version=0.2.2.11
release=1
source=(https://pypi.python.org/packages/ef/9d/6131378948f8a4a0f20a82b5212c3083bfcc06acfe0548976fcc2a98f34b/moviepy-$version.tar.gz)

build()
{
	cd moviepy-$version
	python3 setup.py install --root=$PKG
}
